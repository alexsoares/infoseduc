<font size="2">
<table  class="tabelas" width="50%"align="center">
<tr>
<td>
  <table>
    <tr>
      <td colspan="2" align="left">
         <font  size=5 color="black"> <b> IMPRESSORAS</b></font>
      </td>
     </tr>
  </table>
<fieldset><legend><font  size=5> Dados Impressora</font></legend>
<table width="100%">
  <tr>
    <td align="left" width="20%"><b>Unidade:</b></td>
    <td align="left" colspan="2"><%=h @impressoras.unidade.nome %></td>
  </tr>
  <tr>
    <td align="left"><b>Tipo:</b></td>
    <td align="left"colspan="2"><%=h @impressoras.tipo_controle.nome %></td>
  </tr>
  <tr>
    <td align="left"><b>Configuração:</b></td>
    <td align="left"colspan="2"><%=h @impressoras.config %></td>
  </tr>
  <tr>
    <td align="left"><b>Usuário:</b></td>
    <td align="left"colspan="2"><%=h @impressoras.user %></td>
  </tr>
  <tr>
    <td align="left"><b>Quantidade:</b></td>
    <td align="left"colspan="2"><%=h @impressoras.quant %></td>
  </tr>
  <%if @impressoras.tipo_controle.nome == 'TERCEIROS'%>
  <tr>
    <td align="left"><b>Pertence:</b></td>
    <td align="left"colspan="2"><%=h @impressoras.pertence %></td>
  </tr>
  <tr>
    <td align="left"><b>Contato:</b></td>
    <td align="left"colspan="2"><%=h @impressoras.contato %></td>
  </tr>
  <%else%>
   <tr>
    <td align="left"><b>NºPatrimônio:</b></td>
    <td align="left"colspan="2"><%=h @impressoras.pat %></td>
   </tr>
  <%end%>
  </tr>
 </table>
</fieldset>
    <div class="botao">
   <table>
    <tr>
       <td align="left" width="4%"><%= link_to image_tag("/stylesheets/images/voltar.png", :size => "40x40"),  homes_path, {:method => "get"}, :title => "Voltar"%></td>
        <% if current_user.has_role?('administrador') or current_user.has_role?('estagiario SEDUC') %>
            <td align="left" width="4%" ><%= link_to image_tag("/stylesheets/images/edit.png", :size => "25x25"), edit_impressora_path(@impressoras), :title => "Alterar Dados"%></td>
          <%end%>
       <td align="left" ><%= link_to image_tag("/stylesheets/images/imprimir.png", :size => "30x30"), onclick="javascript:window.print()", :title => "Imprimir" %></td>
    </tr>
  </table>
  </div>



 </table>
